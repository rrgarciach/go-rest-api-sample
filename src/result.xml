<?xml version="1.0"?>
<fetch>
  <entity name="com_sample">
    <attribute name="com_name" alias="samplenumber"/>
    <attribute name="statuscode" alias="statuscode"/>
    <attribute name="com_trfnumber_txt" alias="trfnumber"/>
   <attribute name="ownerid" alias="owner"/>
 <filter type="and">
       <condition attribute="com_trfnumber_txt" operator="in">
      </condition>
       <condition attribute="com_trfnumber_txt" operator="in" value="{notpassed}"/>
      <condition attribute="com_trfnumber_txt" operator="in"><value>trf10000</value><value>trf20000</value></condition>
      <condition attribute="other" operator="not-in"><value>o1</value></condition>
      <condition attribute="onemore" operator="between"><value>trf10000</value><value>trf20000</value></condition>
      <condition attribute="last" operator="not-between"><value>trf10000</value><value>trf20000</value></condition>
     </filter>
  </entity>
</fetch>
